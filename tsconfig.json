{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "jsx": "react",
        "rootDir": "./src",
        "moduleResolution": "node",
        "noEmit": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}